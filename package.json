{
  "name": "medi-xplorer",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "jest --watch",
    "generate-master-versions": "chmod +x ./scripts/generate-versions.sh && ./scripts/generate-versions.sh",
    "generate-master-data": "node --loader ts-node/esm scripts/generate-master-data.ts",
    "build:masters": "npm run generate-master-data && npm run generate-master-versions",
    "pages:build": "npx @cloudflare/next-on-pages",
    "pages:deploy": "npm run pages:build && wrangler pages deploy .vercel/output/static",
    "pages:watch": "npx @cloudflare/next-on-pages --watch",
    "pages:dev": "npx wrangler pages dev .vercel/output/static --compatibility-date=2023-11-21 --compatibility-flag=nodejs_compat",
    "analyze": "ANALYZE=true npm run build"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@tanstack/react-query": "^5.24.1",
    "@tanstack/react-table": "^8.11.6",
    "@tanstack/react-virtual": "^3.1.1",
    "next": "14.1.0",
    "react": "^18",
    "react-dom": "^18",
    "react-select": "^5.8.0",
    "rich-textarea": "^0.24.4",
    "tailwind-merge": "^2.2.1"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.8.6",
    "@next/bundle-analyzer": "^14.1.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-window": "^1.8.8",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "autoprefixer": "^10.4.18",
    "csv-string": "^4.1.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-next-on-pages": "^1.7.4",
    "jest": "^29.7.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "vercel": "^33.5.3"
  }
}
