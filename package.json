    "csv-string": "^4.1.1",
    "next": "14.0.3",
    "react": "^18",
    "react-dom": "^18",
    "react-select": "^5.8.0",
    "react-virtualized-auto-sizer": "^1.0.20",
    "react-window": "^1.8.10",
    "rich-textarea": "^0.24.3",
    "tailwind-merge": "^2.0.0"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.7.4",
    "@types/jest": "^29.5.9",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-virtualized-auto-sizer": "^1.0.3",
    "@types/react-window": "^1.8.8",
    "autoprefixer": "^10.0.1",
