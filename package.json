{
  "name": "medi-xplorer",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "jest --watch",
    "generate-versions": "chmod +x ./scripts/generate-versions.sh && ./scripts/generate-versions.sh",
    "generate-json-const-types": "node --loader ts-node/esm scripts/generate-json-const-types.ts",
    "generate-master-data": "node --loader ts-node/esm scripts/generate-master-data.ts",
    "pages:build": "npx @cloudflare/next-on-pages",
    "pages:deploy": "npm run pages:build && wrangler pages deploy .vercel/output/static",
    "pages:watch": "npx @cloudflare/next-on-pages --watch",
    "pages:dev": "npx wrangler pages dev .vercel/output/static --compatibility-date=2023-11-21 --compatibility-flag=nodejs_compat",
    "analyze": "ANALYZE=true npm run build"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^14.0.4",
    "@tanstack/react-query": "^5.13.4",
    "@tanstack/react-table": "^8.10.7",
    "@tanstack/react-virtual": "^3.0.1",
    "csv-string": "^4.1.1",
    "next": "14.0.4",
    "react": "^18",
    "react-dom": "^18",
    "react-select": "^5.8.0",
    "react-virtualized-auto-sizer": "^1.0.20",
    "rich-textarea": "^0.24.4",
    "tailwind-merge": "^2.0.0"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.8.1",
    "@types/jest": "^29.5.9",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-virtualized-auto-sizer": "^1.0.3",
    "@types/react-window": "^1.8.8",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-next-on-pages": "^1.7.4",
    "iconv-lite": "^0.6.3",
    "jest": "^29.7.0",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "vercel": "^32.5.6"
  }
}